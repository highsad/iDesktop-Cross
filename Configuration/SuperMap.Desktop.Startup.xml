<?xml version="1.0" encoding="utf-8" standalone="no"?>
<startup xmlns="http://www.supermap.com.cn/desktop">
  <!--启动时，是否显示启动画面-->
  <splash enabled="true">
    <script xmlns="" assemblyName="" codeType="CtrlAction" enabled="false" onAction=""/>
    <splashItems xmlns="">
      <backgroundimage image=""/>
      <message bold="true" fontName="宋体" fontSize="9" location="12,251" text="" textColor="1,120,210"/>
    </splashItems>
  </splash>
  <!--工作空间关闭时，是否提示保存-->
  <workspace closenotify="true" visoncheck="true">
  </workspace>
  <!--是否显示工具提示-->
  <ribbon panelShow="true" screenTipShow="true"/>
  <!--窗口的全局选项控制-->
  <dataWindow autoCloseEmptyWindow="false" autoNewWindow="true" autoRemoveEmptyLayer="true" showCloseInfoForm="true">
  </dataWindow>
  <!--有输出提示时，是否自动弹出输出窗口-->
  <output autoPopUp="true">
  </output>
  <!--是否启用用户体验计划-->
  <launchUserExperiencePlan enabled="true">
  </launchUserExperiencePlan>
  <!--是否启用自动更新-->
  <launchAutoUpdate enabled="true">
  </launchAutoUpdate>
  <!--打开地图窗口时，是否显示导航条-->
  <showNavigationBar enabled="false">
  </showNavigationBar>
  <!--打开属性窗口时，是否隐藏系统字段-->
  <hideSysFields enabled="false">
  </hideSysFields>
  <!--属性浏览设置-->
  <PropertyBrowserSetting showDetails="false" showSysFields="false">
  </PropertyBrowserSetting>
  <!--新建场景时，是否自动加载框架数据-->
  <addframedata enabled="false">
  </addframedata>
  <!--新建场景的默认相机位置-->
  <camera altitude="10288740" altitudeMode="relativetoground" heading="0" latitude="36.05" longitude="108.69" tilt="0"/>
  <!--启动SuperMap Desktop .NET时，默认加载的工作环境-->
  <workEnvironment default="Default">
  </workEnvironment>
  <themeStyle default="白色">
  </themeStyle>
  <!--SuperMap iDesktop 的标题及图标-->
  <mainForm icon="" text="SuperMap iDesktop 7C(2015)">
  </mainForm>
  <!--应用程序的区域语言-->
  <culture currentCulture="zh-CN" currentUICulture="zh-CN">
  </culture>
  <!--默认投影配置文件的路径-->
  <projection default="">
  </projection>
  <!--是否自动生成日志，及日志的默认输出路径-->
  <log logFolder="./log/Desktop/" outputToLog="true">
  </log>
  <!--输出信息的级别-->
  <InfoType Exception="true" Information="true">
  </InfoType>
  <!--帮助文档的加载设置-->
  <help onlineAddress="http://support.supermap.com.cn/onlinedoc/deskpronet/" type="1">
  </help>
  <!--编辑回退设置-->
  <edit parameteredit="true" positiveselect="0" rebackitemcount="1000000" rebackitemcountdefine="true" rebacktimes="1000" rebacktimesdefine="true" reverseselect="2">
  </edit>
  <!--是否即使刷新专题图-->
  <theme refresh="true">
  </theme>
  <!--专题图数值框精度(小数点位数)-->
  <precision DecimalPlaces="4">
  </precision>
  <!--SQL查询语句-->
  <!--SQL查询数据集-->
  <SQLDatasetName datasetName="">
  </SQLDatasetName>
  <!--查询字段-->
  <SQLQueryFields queryFields="">
  </SQLQueryFields>
  <!--查询条件-->
  <SQLQueryCondition queryCondition="">
  </SQLQueryCondition>
  <!--分组字段-->
  <SQLQueryGroup groupFields="">
  </SQLQueryGroup>
  <!--字段、排序类型之间以“,”分隔开-->
  <SQLOrderFields orderFields="">
  </SQLOrderFields>
  <SQLOrderType orderType="">
  </SQLOrderType>
  <!-- 最大可见节点数目-->
  <maxVisibleVertex maxCount="3600000">
  </maxVisibleVertex>
  <!-- 捕捉参数设置-->
  <MapControlSnapSettingParameter FixedAngle="90" FixedLength="1000" IsSnappedLineBroken="False" MaxSnappedCount="500" MinSnappedLength="50" Tolerance="3">
  </MapControlSnapSettingParameter>
  <!-- 捕捉模式设置-->
  <MapControlSnapSettingModes LineWithFixedAngle="False" LineWithFixedLength="False" LineWithHorizontalOrVertical="False" LineWithIntersection="True" LineWithParallel="False" LineWithPerpendicular="False" PointOnEndpoint="True" PointOnExtension="False" PointOnLine="True" PointOnMidpoint="False" PointOnPoint="False" PointWithHorizontalOrVertical="False">
  </MapControlSnapSettingModes>
 
</startup>
